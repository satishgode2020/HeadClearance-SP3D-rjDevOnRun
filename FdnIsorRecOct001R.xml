				</Point>
			</Point3D>

			<!--Footing Point-->
			<Point3D>
				<Point ID="FPT1">
					<XYZ X="-(FootingWidth_X/2)" Y="(FootingWidth_Y/2)" Z="- (GroutingThickness + PedestalHeight)" />
